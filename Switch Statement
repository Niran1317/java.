public class switchdaysmonth {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("eneter a month(1-12)");
		int month=scan.nextInt();
		switch(month) {
		case 1:
			System.out.println("january as 31 days");
			break;
		case 2:
			System.out.println("feburay as 28 days");
			break;
		case 3:
			System.out.println("march as 31 days");
		    break;
		case 4:
			System.out.println("april as 30 days");
			break;    
		case 5:
			System.out.println("may as 31 days");
			break;
		case 6:
			System.out.println("june as 30 days");
			break;
		case 7:
			System.out.println("july as 31 days");
			break;
		case 8:
			System.out.println("august as 30 days");
			break;
		case 9:
			System.out.println("september as 31 days");
			break;
		case 10:
			System.out.println("october as 30 days");
			break;
		case 11:
			System.out.println("Novermber as 31 days");
			break;
		case 12:
			System.out.println("december as 30 days");
			break;
			default:
				System.out.println("enter a number");
		}

}
}
