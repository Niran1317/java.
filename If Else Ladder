
public class ifelseladder {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int mark=scan.nextInt();
		char grade;
		if(mark>=90) {
			grade='A'; }
		else if(mark>=80) {
			grade='B'; }
		else if(mark>=70) {
			grade='C'; }
		else if(mark>=50) {
			grade='F'; 
		System.out.println("grade:"+ grade);
		
	}
		
	} }			


