public class arithmetic {
	public static void main(String[] args) {
		int a=15;
		int b=4;
		int add=a+b;
		int sub=a-b;
		int multiple=a*b;
		int divide=a/b;
		int modules=a%b;
		System.out.println(add);
		System.out.println(sub);
		System.out.println(multiple);
		System.out.println(divide);
		System.out.println(modules);
		
	}

}
